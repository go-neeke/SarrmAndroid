<?xml version="1.0" encoding="UTF-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <solid android:color="#ffffffff" />

    <stroke
        android:width="@dimen/outline_stroke_width"
        android:color="@color/outline_color" />

    <padding
        android:bottom="@dimen/outline_padding_vertical"
        android:left="@dimen/outline_padding_horizontal"
        android:right="@dimen/outline_padding_horizontal"
        android:top="@dimen/outline_padding_vertical" />

    <corners
        android:bottomLeftRadius="@dimen/outline_radius"
        android:bottomRightRadius="@dimen/outline_radius"
        android:topLeftRadius="@dimen/outline_radius"
        android:topRightRadius="@dimen/outline_radius" />
</shape>